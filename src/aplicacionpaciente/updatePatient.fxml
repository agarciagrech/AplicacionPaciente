<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aplicacionpaciente.UpdatePatientController">
   <children>
      <Label layoutX="149.0" layoutY="61.0" text="Update MAC Address">
         <font>
            <Font name="Berlin Sans FB" size="34.0" />
         </font>
      </Label>
      <TextField fx:id="MacAddressLabel" layoutX="226.0" layoutY="128.0" promptText="MAC Address" />
      <Button fx:id="UpdateButton" layoutX="130.0" layoutY="128.0" mnemonicParsing="false" onAction="#Update" text="Update" />
      <Button fx:id="backButton" layoutX="17.0" layoutY="11.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <Button fx:id="exitButton" layoutX="556.0" layoutY="11.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
   </children>
</AnchorPane>
