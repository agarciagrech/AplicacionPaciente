<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="571.0" prefWidth="818.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aplicacionpaciente.ShowSignalController">
   <children>
      <LineChart fx:id="Graph" layoutX="-248.0" layoutY="48.0" prefHeight="523.0" prefWidth="1118.0" scaleX="0.5" scaleY="0.8">
        <xAxis>
          <NumberAxis side="BOTTOM" tickLabelGap="1.0" upperBound="6000.0" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" tickLabelGap="1.0" upperBound="1000.0" />
        </yAxis>
      </LineChart>
      <Button fx:id="DisplayButton" layoutX="710.0" layoutY="326.0" mnemonicParsing="false" onAction="#Display" text="Display" />
      <Button fx:id="consultFilenames" layoutX="687.0" layoutY="162.0" mnemonicParsing="false" onAction="#showFilenames" text="Consult Filenames " />
      <TextField fx:id="txtFIlename" layoutX="670.0" layoutY="244.0" promptText="Signal Filename" />
      <Button fx:id="exitButton" layoutX="820.0" layoutY="14.0" mnemonicParsing="false" onAction="#exitApp" text="Exit" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#back" text="Back " />
   </children>
</AnchorPane>
